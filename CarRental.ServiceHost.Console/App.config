<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
 
  <connectionStrings>
    <add name="CarRental" connectionString="Data Source=SYNSANMAT-LEN2\SQLEXPRESS;Initial Catalog=CarRental;Integrated Security=True" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <entityFramework>
   
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer" />
    </providers>
    
    
  </entityFramework>
  <system.serviceModel>
    <services>
      <service name="CarRental.Business.Managers.Managers.InventoryManager" >
        <endpoint address="net.tcp://localhost:8090/InventoryService"
                  binding="netTcpBinding"
                  contract="CarRental.Business.Contracts.IInventoryService">          
        </endpoint>        
      </service>
      <service name="CarRental.Business.Managers.Managers.AccountManager">
        <endpoint address="net.tcp://localhost:8091/AccountService"
                  binding="netTcpBinding"
                  contract="CarRental.Business.Contracts.IAccountService">          
        </endpoint>
      </service>
      <service name="CarRental.Business.Managers.RentalManager">
        <endpoint address="net.tcp://localhost:8092/RentalService"
                  binding="netTcpBinding"
                  contract="CarRental.Business.Contracts.IRentalService">
        </endpoint>
      </service>
  </services>

    <bindings>
      <netTcpBinding>
        <binding transactionFlow="true" maxReceivedMessageSize="2000000">
          <reliableSession enabled="true"/>
        </binding>
      </netTcpBinding>
    </bindings>
  
  </system.serviceModel>
  
</configuration>