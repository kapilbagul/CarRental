
@{
    ViewBag.Title = "Account Registration";
}



@section scripts
{
    
<script src="~/Bindings/Models/AccountRegisterModel.js"></script>
<script src="~/Bindings/ViewModels/AccountRegisterViewModel.js"></script>
}

@section footerScript
{
    $("#registration").data("$injector","");
    angular.bootstrap(document.getElementById("registration"),['accountRegister'])
}

<h2>Register</h2>

<div ng-non-bindable >
    <div  ng-controller="AccountRegisterViewModel"  id="registration">
        <br />
        <div>
            @Html.Partial("~/Views/Shared/_ValidationErrors.cshtml")
            <br />
        </div>
        <div ng-view></div>

    </div>
</div>
